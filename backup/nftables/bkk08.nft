		type nat hook postrouting priority srcnat; policy accept;
		ip saddr 10.8.0.0/16 oifname "vmbr2" snat to 160.22.181.8
		ip saddr 192.168.0.0/16 oifname "vmbr2" snat to 160.22.181.8
	}
